{
  "apps": {
    "name": "service.Ayo",
    "script": "dotnet",
    "exec_mode": "fork",
    "error_file": "./log/err.log",
    "out_file": "./log/out.log",
    "merge_logs": true,
    "log_date_format": "YYYY/MM/DD HH:mm:ss",
    "min_uptime": "60s",
    "max_restarts": 30,
    "autorestart": true,
    "restart_delay": "60",
    "args": [
      "Ayo.API.dll",
      "--urls=http://*:7894",
      "--env=qa"
    ]
  }
}